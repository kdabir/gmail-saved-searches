{
  "name": "GMail Saved Searches",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Save your gmail search queries and quickly access when required",
  "homepage_url": "https://github.com/kdabir/gmail-saved-searches",
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_popup": "action.html",
    "default_title": "GMail Saved Searches",
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://mail.google.com/*",
    "tabs",
    "storage",
    "declarativeContent"
  ]
}
